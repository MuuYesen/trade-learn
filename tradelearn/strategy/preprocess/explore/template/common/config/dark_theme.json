{
    "figure": {
        "figsize": [
            30,
            10
        ],
        "dpi": 150,
        "facecolor": "#1e1e1e",
        "edgecolor": "#1e1e1e"
    },
    "axes": {
        "grid": true,
        "labelpad": 7.0,
        "labelsize": 20,
        "titlesize": 24,
        "titlepad": 14.0,
        "titleweight": "bold",
        "titlecolor": "#FFFFFF",
        "facecolor": "#121212",
        "tick_params": {
            "labelsize": 16,
            "labelrotation": 0,
            "color": "#FFFFFF"
        }
    },
    "font": {
        "family": "serif",
        "serif": "Times New Roman",
        "color": "#FFFFFF"
    },
    "grid": {
        "linewidth": 1,
        "color": "#444444"
    },
    "ticks": {
        "xtick": {
            "labelsize": 16,
            "major_pad": 4,
            "color": "#FFFFFF"
        },
        "ytick": {
            "labelsize": 16,
            "major_pad": 4,
            "color": "#FFFFFF"
        }
    },
    "legend": {
        "fontsize": 16,
        "facecolor": "#1e1e1e",
        "framealpha": 1,
        "edgecolor": "#444444",
        "title_fontsize": 18,
        "shadow": true,
        "fancybox": true,
        "textcolor": "#FFFFFF"
    },
    "lines": {
        "linewidth": 3
    },
    "text": {
        "color": "#FFFFFF"
